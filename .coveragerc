# .coveragerc
[run]
source = .
branch = True
omit =
    */migrations/*
    */tests/*
    manage.py
    */__init__.py
    */check_email_app/admin.py
    */check_email_app/apps.py
    */check_email_app/models.py
    */videoflix_backend\asgi.py
    */videoflix_backend\wsgi.py

[report]
fail_under = 80
show_missing = True
skip_covered = True
